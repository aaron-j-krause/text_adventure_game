{
  "extends":"airbnb",
  "rules" : {
    "semi": [0, "never"],
    "react/jsx-filename-extension": 0,
    "import/no-extraneous-dependencies": 0,
    "comma-dangle": 0,
    "react/require-default-props": 0,
    "consistent-return": 0,
    "no-unused-vars": ["error", {"argsIgnorePattern": "^_"}],
    "no-param-reassign": 0,
    "no-use-before-define": 0
  },
  "env": {
    "es6": true,
    "node": true,
	"jest": true
  },
  "globals": {
    "document": true
  }
}